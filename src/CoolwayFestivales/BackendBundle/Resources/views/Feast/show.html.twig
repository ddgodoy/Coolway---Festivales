

<table class="table table-striped">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Nombre</th>
            <td>{{ entity.name }}</td>
        </tr>
        <tr>
            <th>Latitud</th>
            <td>{{ entity.latitude }}</td>
        </tr>
        <tr>
            <th>Longitude</th>
            <td>{{ entity.longitude }}</td>
        </tr>
        <tr>
            <th>Desde</th>
            <td>{{ entity.date_from | date("d-m-Y") }}</td>
        </tr>
        <tr>
            <th>Hasta</th>
            <td>{{ entity.date_to | date("d-m-Y") }}</td>
        </tr>

    </tbody>
</table>