

<table class="table table-striped">
    <tbody>
        <tr>

            <td colspan="2"><img class="img-responsive" src="{{asset(entity.getWebPath)}}" alt="{{entity.name}}" title="{{entity.name}}"/></td>
        </tr>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Festival</th>
            <td>{{ entity.feast }}</td>
        </tr>
        <tr>
            <th>Usuario</th>
            <td>{{ entity.user }}</td>
        </tr>
        <tr>
            <th>Total</th>
            <td>{{ entity.total }}</td>
        </tr>
        <tr>
            <th>Dance</th>
            <td>{{ entity.dance }}</td>
        </tr>
        <tr>
            <th>Musica</th>
            <td>{{ entity.music }}</td>
        </tr>
        <tr>
            <th>Total Share</th>
            <td>{{ entity.getTotalShare }}</td>
        </tr>
        <tr>
            <th>Latitud</th>
            <td>{{ entity.latitude }}</td>
        </tr>
        <tr>
            <th>Longitud</th>
            <td>{{ entity.longitude }}</td>
        </tr>



    </tbody>
</table>